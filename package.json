{
  "name": "stylelint-config-aesthetic-coder",
  "version": "1.0.2",
  "description": "My personal stylelint configuration",
  "author": "Luis Monsalve",
  "license": "MIT",
  "keywords": [
    "stylelint",
    "stylelint-config",
    "stylelint-order",
    "order"
  ],
  "homepage": "https://github.com/AesthetiCoder/stylelint_coder_dev",
  "bugs": {
    "url": "https://github.com/AesthetiCoder/stylelint_coder_dev/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AesthetiCoder/stylelint_coder_dev.git"
  },
  "files": [
    "./dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./sass": {
      "require": "./dist/sass.js",
      "import": "./dist/sass.mjs",
      "types": "./dist/sass.d.ts"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "^18.16.0",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": ">=8.2.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "git config core.hooksPath ./script/git_hook",
    "lint": "eslint --ext .js, .ts . --fix",
    "build": "rimraf dist && tsc",
    "type-check": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "postcss": "^8.4.24",
    "postcss-scss": "^4.0.6",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.0.1",
    "stylelint-a11y": "^1.2.3"
  },
  "devDependencies": {
    "eslint": "^8.39.0",
    "eslint-config-aesthetic-coder": "^1.0.4",
    "rimraf": "^5.0.1",
    "stylelint": "^15.5.0"
  },
  "peerDependencies": {
    "stylelint": "^15.5.0"
  }
}